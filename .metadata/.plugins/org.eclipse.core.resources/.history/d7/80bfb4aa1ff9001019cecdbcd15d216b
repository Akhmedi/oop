package oop;
import java.util.Scanner;

public class Main {
  public static void main(String[] args) {
	 Scanner scanner = new Scanner(System.in);
	 System.out.print("Введите имя: ");
     String name = scanner.nextLine();
     
     for(int i = 0;i < 5;i++) {
    	 System.out.print(name);
     }
     
  }
}
